{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/.sass-cache": true,
        "**/_site": true,
        "**/node_modules": true
    },
    "editor.rulers": [80],
    "editor.quickSuggestions": false,
    "cSpell.ignoreRegExpList": [
        "/`.*?`/",
        "/```(\\r|\\n|.)*```/gm"
    ]
}